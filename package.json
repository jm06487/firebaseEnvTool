{
  "name": "firebase-env-cli",
  "version": "1.0.0",
  "description": "A CLI tool for managing Firebase environment variables",
  "main": "src/main.ts",
  "bin": {
    "fireEnv": "./src/main.ts"
  },
  "scripts": {
    "start": "node dist/bundle.js",
    "build": "esbuild ./src/main.ts --bundle --platform=node --outdir=./dist --format=cjs --target=es2020",
    "test": "ava"
  },
  "keywords": [
    "firebase",
    "environment",
    "CLI"
  ],
  "author": "Jaime Mac Gregor",
  "license": "MIT",
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "fkill": "^9.0.0",
    "inquirer": "^9.2.15",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@rollup/plugin-json": "^6.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/shelljs": "^0.8.15",
    "ava": "^6.1.2",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.20.1",
    "esm": "^3.2.25",
    "jest": "^29.7.0",
    "rollup": "^4.12.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "tests/**/*.test.ts"
    ]
  }
}
