{
  "name": "firebase-env-cli",
  "version": "1.0.0",
  "description": "A CLI tool for managing Firebase environment variables",
  "main": "src/main.ts",
  "type": "module",
  "bin": {
    "fireEnv": "./dist/cli.prod.js"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.ts",
    "start": "node dist/cli.prod.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest --ui"
  },
  "keywords": [
    "firebase",
    "environment",
    "CLI"
  ],
  "author": "Jaime Mac Gregor",
  "license": "MIT",
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "fkill": "^9.0.0",
    "inquirer": "^9.2.15",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@logtail/node": "^0.4.19",
    "@types/eventemitter3": "^2.0.2",
    "@types/inquirer": "^9.0.7",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
